  {
  let guestList: string[] = ["Leonardo da Vinci", "Marie Curie", "Nelson Mandela"];
  const unableToAttend = guestList.shift();
  console.log(`${unableToAttend} regrets they can't make it.`);
  guestList.push("Ada Lovelace");
  guestList.forEach(guest => console.log(`Dear ${guest}, your presence would make the evening truly special.`));
  }
  
  
  {
  let guestList: string[] = ["Leonardo da Vinci", "Marie Curie", "Nelson Mandela"];
  const unableToAttend = guestList.shift();
  guestList.push("Ada Lovelace");
  console.log("Great news! We've found a bigger dinner table!");
  guestList.unshift("Isaac Newton");
  guestList.splice(Math.floor(guestList.length / 2), 0, "Albert Einstein", "New Guest", "Stephen Hawking");
  guestList.forEach(guest => console.log(`Dear ${guest}, your presence would make the evening truly special.`));
  }
  
  
  {
  let guestList: string[] = ["Leonardo da Vinci", "Marie Curie", "Nelson Mandela", "Ada Lovelace", "Isaac Newton", "Albert Einstein", "New Guest", "Stephen Hawking"];
  console.log("Apologies, the new dinner table won't arrive in time. We can only invite two people for dinner.");
  while (guestList.length > 2) {
      const removedGuest = guestList.pop();
      console.log(`Sorry, ${removedGuest}, there's been a change and we can't accommodate you for dinner.`);
  }
  guestList.forEach(guest => console.log(`Dear ${guest}, you're still invited to the dinner.`));
  guestList.length = 0; // Empty the list
  console.log("Final Guest List:", guestList);
  }
  
  
  {
  let travelDestinations: string[] = ["Japan", "Iceland", "Australia", "Peru", "Greece"];
  console.log("Original Order:", travelDestinations);
  console.log("Alphabetical Order:", [...travelDestinations].sort());
  console.log("Original Order (after sorting):", travelDestinations);
  console.log("Reverse Alphabetical Order:", [...travelDestinations].sort().reverse());
  console.log("Original Order (after reverse sorting):", travelDestinations);
  travelDestinations.reverse();
  console.log("Reversed Order:", travelDestinations);
  travelDestinations.reverse();
  console.log("Original Order (after double reverse):", travelDestinations);
  console.log("Sorted in Alphabetical Order:", travelDestinations.sort());
  console.log("Sorted in Reverse Alphabetical Order:", travelDestinations.sort().reverse());
  }
  
  
