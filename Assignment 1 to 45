  {
  let car: string = 'subaru';
  let number: number = 10;
  console.log("Is car == 'subaru'? I predict True.", car == 'subaru');
  console.log("Is number < 20? I predict True.", number < 20);
  console.log("Is number >= 10? I predict True.", number >= 10);
  console.log("Is car != 'ford'? I predict True.", car != 'ford');
  console.log("Is number === 10? I predict True.", number === 10);
  console.log("Is car == 'honda'? I predict False.", car == 'honda');
  console.log("Is number > 100? I predict False.", number > 100);
  console.log("Is number <= 5? I predict False.", number <= 5);
  console.log("Is car === 'Subaru'? I predict False.", car === 'Subaru');
  console.log("Is number !== 10? I predict False.", number !== 10);
  }
  
  
  {
  let fruit: string = 'apple';
  console.log("Is fruit == 'apple'? I predict True.", fruit == 'apple');
  console.log("Is fruit != 'banana'? I predict True.", fruit != 'banana');
  let name: string = 'John';
  console.log("Is name.toLowerCase() == 'john'? I predict True.", name.toLowerCase() == 'john');
  let age: number = 25;
  console.log("Is age === 25? I predict True.", age === 25);
  console.log("Is age !== 30? I predict True.", age !== 30);
  console.log("Is age > 20? I predict True.", age > 20);
  console.log("Is age < 30? I predict True.", age < 30);
  console.log("Is age >= 25? I predict True.", age >= 25);
  console.log("Is age <= 25? I predict True.", age <= 25);
  let x: number = 10;
  console.log("Is x > 5 and x < 15? I predict True.", x > 5 && x < 15);
  console.log("Is x < 5 or x > 15? I predict False.", x < 5 || x > 15);
  let colors: string[] = ['red', 'green', 'blue'];
  console.log("Is 'green' in colors? I predict True.", colors.includes('green'));
  console.log("Is 'yellow' not in colors? I predict True.", !colors.includes('yellow'));
  }
  
  {
  let alien_color_pass: string = 'green';
  if (alien_color_pass === 'green') {
      console.log("The player just earned 5 points.");
  }
  
  // Failing the If Test (no output)
  let alien_color_fail: string = 'red'; // Not 'green'
  if (alien_color_fail === 'green') {
      console.log("The player just earned 5 points."); // This won't be printed as the color is not 'green'
  }
  }
  
  
  {
  // Running the if Block (Alien is Green)
  let alien_color_if: string = 'green';
  if (alien_color_if === 'green') {
      console.log("The player just earned 5 points for shooting the alien.");
  } else {
      console.log("The player just earned 10 points.");
  }
  
  // Running the else Block (Alien is Not Green)
  let alien_color_else: string = 'red'; // Not 'green'
  if (alien_color_else === 'green') {
      console.log("The player just earned 5 points for shooting the alien.");
  } else {
      console.log("The player just earned 10 points."); // This will be printed as the color is not 'green'
  }
  }
  
  
  {
  let alien_color: string;
  
  alien_color = 'green';
  if (alien_color === 'green') {
      console.log("The player just earned 5 points.");
  } else if (alien_color === 'yellow') {
      console.log("The player just earned 10 points.");
  } else {
      console.log("The player just earned 15 points.");
  }
  
  alien_color = 'yellow';
  if (alien_color === 'green') {
      console.log("The player just earned 5 points.");
  } else if (alien_color === 'yellow') {
      console.log("The player just earned 10 points.");
  } else {
      console.log("The player just earned 15 points.");
  }
  
  alien_color = 'red';
  if (alien_color === 'green') {
      console.log("The player just earned 5 points.");
  } else if (alien_color === 'yellow') {
      console.log("The player just earned 10 points.");
  } else {
      console.log("The player just earned 15 points.");
  }
  }
  
  
  {
  let age: number = 30;
  if (age < 2) {
      console.log("The person is a baby.");
  } else if (age >= 2 && age < 4) {
      console.log("The person is a toddler.");
  } else if (age >= 4 && age < 13) {
      console.log("The person is a kid.");
  } else if (age >= 13 && age < 20) {
      console.log("The person is a teenager.");
  } else if (age >= 20 && age < 65) {
      console.log("The person is an adult.");
  } else {
      console.log("The person is an elder.");
  }
  }
  
  
