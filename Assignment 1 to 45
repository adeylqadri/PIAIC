  {
  function make_shirt(size: string = 'L', message: string = 'I love TypeScript'): void {
      console.log(`The shirt is size ${size} and it will have the message: "${message}".`);
  }
  make_shirt(); // Large shirt with default message
  make_shirt('M'); // Medium shirt with default message
  make_shirt('S', 'Hello World!'); // Small shirt with a different message
  }
  
  
  {
  function describe_city(city: string, country: string = 'Unknown'): void {
      console.log(`${city} is in ${country}.`);
  }
  describe_city('Karachi', 'Pakistan'); // City and country provided
  describe_city('London', 'UK'); // City and country provided
  describe_city('Paris'); // City provided, default country used
  }
  
  
  {
  function city_country(city: string, country: string): string {
      return `${city}, ${country}`;
  }
  // Calling the function and printing the returned values
  console.log(city_country('Lahore', 'Pakistan'));
  console.log(city_country('New York', 'USA'));
  console.log(city_country('Sydney', 'Australia'));
  }
  
  
  {
  function make_album(artist: string, title: string, tracks?: number): { artist: string, title: string, tracks?: number } {
      let album = { artist: artist, title: title };
      if (tracks !== undefined) {
          album.tracks = tracks;
      }
      return album;
  }
  console.log(make_album('Artist1', 'Title1')); // Album without track information
  console.log(make_album('Artist2', 'Title2', 12)); // Album with track information
  console.log(make_album('Artist3', 'Title3')); // Another album without track information
  }
  
  
  {
  function show_magicians(magicians: string[]): void {
      for (let magician of magicians) {
          console.log(magician);
      }
  }
  let magician_names: string[] = ['Merlin', 'Harry Houdini', 'David Copperfield', 'Penn Jillette', 'Teller'];
  show_magicians(magician_names);
  }
  
  
  {
  function show_magicians(magicians: string[]): void {
      for (let magician of magicians) {
          console.log(magician);
      }
  }
  function make_great(magicians: string[]): string[] {
      return magicians.map(magician => `the Great ${magician}`);
  }
  let magician_names: string[] = ['Merlin', 'Harry Houdini', 'David Copperfield', 'Penn Jillette', 'Teller'];
  let great_magicians = make_great(magician_names);
  show_magicians(great_magicians);
  }
  
  
  {
  function show_magicians(magicians: string[]): void {
      for (let magician of magicians) {
          console.log(magician);
      }
  }
  function make_great(magicians: string[]): string[] {
      return magicians.map(magician => `the Great ${magician}`);
  }
  let magician_names: string[] = ['Merlin', 'Harry Houdini', 'David Copperfield', 'Penn Jillette', 'Teller'];
  let great_magicians = make_great([...magician_names]); // Using the spread operator to create a copy
  show_magicians(magician_names); // Displaying original names
  show_magicians(great_magicians); // Displaying the modified names
  }
  
   
  {
  function make_sandwich(...items: string[]): void {
      console.log("Creating a sandwich with:");
      for (let item of items) {
          console.log("- " + item);
      }
  }
  make_sandwich('Ham', 'Cheese', 'Lettuce'); // Call 1
  make_sandwich('Turkey', 'Bacon', 'Tomato', 'Mayonnaise'); // Call 2
  make_sandwich('Peanut Butter', 'Jelly'); // Call 3
  }
  
  
  {
  interface CarInfo {
      manufacturer: string;
      model: string;
      [key: string]: any;
  }
  function carDetails(manufacturer: string, model: string, ...otherDetails: [string, any][]): CarInfo {
      const car: CarInfo = { manufacturer, model, ...Object.fromEntries(otherDetails) };
      return car;
  }
  const myCar = carDetails('Toyota', 'Corolla', ['color', 'red'], ['year', 2022]);
  console.log(myCar);
  }
  
  
